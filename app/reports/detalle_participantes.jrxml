<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="detalle_participantes" language="groovy" pageWidth="255" pageHeight="752" columnWidth="255" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="3.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="COMPARECENCIA" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     IFNULL(participantes.`razon_social`, CONCAT(participantes.nombre, ' ', participantes.paterno, ' ', participantes.materno)) as nombre_participante,
     participantes.perfil
FROM
     participantes
WHERE
     comparecencia_id = $P{COMPARECENCIA}
ORDER BY
     perfil DESC,
     razon_social DESC,
     paterno ASC,
     materno ASC,
     nombre ASC
]]>
	</queryString>
	<field name="nombre_participante" class="java.lang.String">
		<fieldDescription><![CDATA[]]></fieldDescription>
	</field>
	<field name="perfil" class="java.lang.String">
		<fieldDescription><![CDATA[]]></fieldDescription>
	</field>
	<background>
		<band splitType="Stretch"/>
	</background>
	<columnHeader>
		<band height="3" splitType="Stretch"/>
	</columnHeader>
	<detail>
		<band height="10" splitType="Stretch">
			<textField isStretchWithOverflow="true">
				<reportElement isPrintRepeatedValues="false" x="0" y="0" width="255" height="10"/>
				<textElement markup="html">
					<font size="8"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{nombre_participante} + " " + "(" +  $F{perfil} + ")"]]></textFieldExpression>
			</textField>
		</band>
	</detail>
	<pageFooter>
		<band height="4" splitType="Stretch"/>
	</pageFooter>
</jasperReport>
