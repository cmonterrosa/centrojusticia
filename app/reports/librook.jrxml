	<queryString>
		<![CDATA[SELECT
	t.id, t.fechahora, UPPER(CONCAT(t.folio_expediente, ' / ', t.anio)) as numero_expediente, t.created_at,
	DATE_FORMAT(c.created_at,'%d/%m/%Y %h:%i %p') as fecha_conclusion,
	UPPER(CONCAT(m.fundamento, ' / ', m.causa)) as causa_fundamento,
        mat.descripcion as materia,
        mec.descripcion as mecanismo_alternativo,
        comp.asunto, comp.id as comparecencia_id
FROM tramites t

LEFT JOIN mecanismo_alternativos mec
	ON t.mecanismo_alternativo_id=mec.id
LEFT JOIN concluidos c
	ON t.id = c.tramite_id
LEFT JOIN motivo_conclusions m
	ON c.motivo_conclusion_id=m.id
LEFT JOIN comparecencias comp
	ON t.id = comp.tramite_id
LEFT JOIN materias mat
	ON mat.id=t.materia_id
WHERE
	(t.anio IS NOT NULL AND t.anio = $P{P_ANIO}) AND
        (t.folio_expediente IS NOT NULL) AND
        (t.created_at between $P{P_INICIO} AND $P{P_FIN})
ORDER BY
	t.anio, t.folio_expediente]]>
	</queryString>
