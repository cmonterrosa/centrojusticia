<h2>Listado de usuarios</h2><br />
<!-- contenido -->
<div id="divlista">
    <table width="85%" border="0" cellspacing="0" cellpadding="0" class="sample" style="BORDER-RIGHT: #cccccc 1px solid; BORDER-TOP: #cccccc 1px solid; BORDER-LEFT: #cccccc 1px solid; BORDER-BOTTOM: #cccccc 1px solid">
    <tr class="d0">
       <td style="vertical-align: middle;">Especialista</td>
       <td style="vertical-align: middle;">Número de orientaciones de los últimos 7 días</td>
    </tr>
    <%@especialistas.each do |e|%>
    <tr class="d1">
      <td style="vertical-align: middle;"><%=h(e.nombre_completo)%></td>
      <td style="vertical-align: middle;"><%=h(e.num_orientaciones_por_semana)%></td>
     
     </tr>
    <%end%>
  </table>
</div>