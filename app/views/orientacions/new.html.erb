  <p>
    <%= f.label :fechahora %><br />
    <%= f.datetime_select :fechahora %>
  </p>
  <p>
    <%= f.label :observaciones %><br />
    <%= f.text_field :observaciones %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :sala_id %><br />
