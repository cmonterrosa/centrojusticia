<div id="divlista">
  <table width="95%" border="0" cellspacing="0" cellpadding="0" class="sample" style="BORDER-RIGHT: #cccccc 1px solid; BORDER-TOP: #cccccc 1px solid; BORDER-LEFT: #cccccc 1px solid; BORDER-BOTTOM: #cccccc 1px solid">
    <tr class="d0">
       <td style="vertical-align: middle;">Fecha/Hora</td>
        <td style="vertical-align: middle;">Nombre</td>
        <td style="vertical-align: middle;">Mediador</td>
        <td style="vertical-align: middle;">Comediador</td>
        
        <td style="vertical-align: middle;"><%=h "Estatus"%></td>
        <td style="vertical-align: middle;" colspan="5">Acciones</td>
    </tr>
    <%@sesiones.each do |sesion|%>
    <tr>
       <td style="vertical-align: middle;"><%=sesion.start_at%></td>
       <td style="vertical-align: middle;"><%=sesion.descripcion%></td>
       <td style="vertical-align: middle;">---</td>
       <td style="vertical-align: middle;">---</td>
       <td style="vertical-align: middle;"><%=sesion.estatus_sesion%></td>
    </tr>
    <%end%>
  </table>
</div>
