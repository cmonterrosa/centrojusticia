<!-- Historial de sesiones-->
<%@concluidos = Concluido.find(:all, :conditions => ["tramite_id = ?", @tramite.id], :order => "created_at")%>
<%unless @concluidos.empty?%>
    <table width="80%" border="0" cellspacing="0" cellpadding="0" class="sample" style="BORDER-RIGHT: #cccccc 1px solid; BORDER-TOP: #cccccc 1px solid; BORDER-LEFT: #cccccc 1px solid; BORDER-BOTTOM: #cccccc 1px solid">
      <tr style="background-color:gray; color:white">
        <th colspan="3">
          Detalle de conclusión de trámite/expediente
        </th>
      </tr>
      <tr class="d0">
          <td style="vertical-align: middle;"><b>Concluido por:</b></td>
          <td style="vertical-align: middle;"><b>Motivo</b></td>
          <td style="vertical-align: middle;"><b>Fecha/hora</b></td>
      </tr>
      <%@concluidos.each do |row|%>
        <tr class="d1">
          <td style="vertical-align: middle;"><%=h (row.user_id) ? row.user.nombre_completo : "----"%></td>
          <td style="vertical-align: middle;"><%=h (row.motivo_conclusion) ? row.motivo_conclusion.descripcion_completa : "---"%></td>
          <td style="vertical-align: middle;"><%=h(row.created_at ? row.created_at.strftime("%d de %B de %Y - %H:%M %p") : "---")%></td>
        </tr>
     <%end%>
  </table><br /><br />
  <%end%>
